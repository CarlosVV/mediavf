<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="fileTypeConfiguration" type="MediaVF.CodeSync.FileTypeConfiguration, MediaVF.CodeSync"/>
  </configSections>

  <fileTypeConfiguration>
    <fileType name="CSharp Code Files">
      <add extension="cs" />
      <add extension="resx" />
      <add extension="xaml" />
    </fileType>
    <fileType name=".NET Project Files">
      <add extension="csproj" />
      <add extension="sln" />
    </fileType>
    <fileType name="Web Files">
      <add extension="html" />
      <add extension="htm" />
      <add extension="aspx" />
      <add extension="asp" />
    </fileType>
    <fileType name="XML Files">
      <add extension="xml" />
      <add extension="config" />
    </fileType>
    <fileType name="Binary Files">
      <add extension="exe" />
      <add extension="dll" />
    </fileType>
    <fileType name="Image Files">
      <add extension="png" />
      <add extension="jpg" />
      <add extension="jpeg" />
      <add extension="ico" />
      <add extension="gif" />
    </fileType>
  </fileTypeConfiguration>
</configuration>