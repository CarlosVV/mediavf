    <registration type="AutoTrade.MarketData.Yahoo.Csv.ICsvColumnProviderFactory, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.Csv.CsvColumnProviderFactory, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
    <registration type="AutoTrade.MarketData.Yahoo.Csv.ICsvUrlProvider, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.Csv.CsvUrlProvider, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
    <registration type="AutoTrade.MarketData.Yahoo.Csv.ICsvResultTranslator, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.Csv.CsvResultTranslator, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
