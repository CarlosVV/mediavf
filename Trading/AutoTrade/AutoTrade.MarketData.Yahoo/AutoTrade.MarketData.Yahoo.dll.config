<assemblyConfiguration>
  <settings>
    <setting name="YqlEndpoint" value="http://query.yahooapis.com/v1/public/yql?q={0}&amp;env=store%3A%2F%2Fdatatables.org%2Falltableswithkeys" />
    <setting name="YqlIncludeDiagnostics" value="true" />
    <setting name="YqlMultiStockQuoteSelect" value="select * from yahoo.finance.quotes where symbol in ({0})" />
    <setting name="CsvEndpoint" value="http://finance.yahoo.com/d/quotes.csv?s={0}&amp;f={1}" />
  </settings>
  <registrations>
    <registration type="AutoTrade.MarketData.Yahoo.IYahooMarketDataSettings, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.YahooMarketDataAppSettings, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />

