                  type="AutoTrade.MarketData.Yahoo.IYahooMarketDataProvider, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.Csv.CsvMarketDataProvider, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
    <registration type="System.Collections.Generic.IEnumerable`1[[AutoTrade.MarketData.Yahoo.IYahooMarketDataProvider, AutoTrade.MarketData.Yahoo]], mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
                  mapTo="AutoTrade.MarketData.Yahoo.IYahooMarketDataProvider[], AutoTrade.MarketData.Yahoo"
                  lifetime="singleton" />

    <registration name="YahooMarketDataProvider"
                  type="AutoTrade.MarketData.IMarketDataProvider, AutoTrade.MarketData"
                  mapTo="AutoTrade.MarketData.Yahoo.YahooMarketDataProvider, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
  </registrations>
  
