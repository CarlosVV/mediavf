                  mapTo="AutoTrade.MarketData.Yahoo.Yql.YqlQueryProvider, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
    <registration type="AutoTrade.MarketData.Yahoo.Yql.IYqlUrlProvider, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.Yql.YqlUrlProvider, AutoTrade.MarketData.Yahoo"
                  lifetime="instance" />
    <registration type="AutoTrade.MarketData.Yahoo.Yql.IYqlResultTranslator, AutoTrade.MarketData.Yahoo"
                  mapTo="AutoTrade.MarketData.Yahoo.Yql.YqlResultTranslator, AutoTrade.MarketData.Yahoo"
